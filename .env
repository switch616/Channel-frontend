# 环境标识
VITE_APP_ENV=development

# 后端 API 地址（FastAPI 接口地址）
VITE_API_BASE_URL=http://localhost:8088/api/v1/

# 项目名称（可用于显示在标题等）
VITE_APP_TITLE=YUN 社交媒体平台
VITE_APP_DESCRIPTION=YUN 是一个年轻人聚集的社交内容平台，支持视频创作、粉丝互动与创作者变现。
VITE_APP_KEYWORDS=社交平台, 视频创作, YUN, 短视频, 年轻人社区, 创作者经济

# 版本号（方便前端展示或调试）
VITE_APP_VERSION=1.0.0

# 其他自定义配置（如是否启用调试）
VITE_DEBUG=true

# 开发环境配置
VITE_DEV_MODE=true

# 性能监控配置
# VITE_ENABLE_PERFORMANCE_MONITOR=true

# 缓存配置
VITE_CACHE_DURATION=300000

# 预加载配置
VITE_ENABLE_PRELOAD=true

